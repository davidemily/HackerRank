//Complete the following function.

int marks_summation(int* marks, int number_of_students, char gender) {
  //Write your code here.
    int result=0, i=0;
    if(gender == 'g')
    {
        i++;
    }
    for(i; i<number_of_students && marks[i]; i++,i++)
    {
        result = result+marks[i];
    }
    return result;
}
